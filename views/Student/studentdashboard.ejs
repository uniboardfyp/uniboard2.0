<%- include('partials/studenthead') %>
<div class="container-fluid py-4 min-vh-100">
    <div class="row">
      <div class="col-xl-4 col-sm-6 mb-xl-0 mb-4">
        <div class="card">
          <div class="card-body p-3">
            <div class="row">
              <div class="col-8">
                <div class="numbers">
                  <p class="text-md mb-0 text-uppercase font-weight-bold">Active Courses</p>
                  <br>
                  <h5 class="font-weight-bolder">
                    <%= totalActiveCourses %>
                  </h5>
                 
                </div>
              </div>
              <div class="col-4 text-end">
                <div class="icon icon-shape bg-gradient-success shadow-success text-center rounded-circle">
                  <i class="ni ni-paper-diploma text-lg opacity-10" aria-hidden="true"></i>
                </div>
                
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-xl-4 col-sm-6 mb-xl-0 mb-4">
        <div class="card">
          <div class="card-body p-3">
            <div class="row">
              <div class="col-8">
                <div class="numbers">
                  <p class="text-md mb-0 text-uppercase font-weight-bold">Courses Completed</p>
                  <br>
                  <h5 class="font-weight-bolder">
                    <%= totalCompletedCourses %>
                  </h5>
                 
                </div>
              </div>
              <div class="col-4 text-end">
                <div class="icon icon-shape bg-gradient-danger shadow-danger text-center rounded-circle">
                  <i class="ni ni-world text-lg opacity-10" aria-hidden="true"></i>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-xl-4 col-sm-6 mb-xl-0 mb-4">
        <div class="card">
          <div class="card-body p-3">
            <div class="row">
              <div class="col-8">
                <div class="numbers">
                  <p class="text-md mb-0 text-uppercase font-weight-bold">Account Balance</p>
                  <br>
                  <h5 class="font-weight-bolder">
                    Rs 3,462
                  </h5>
                
                </div>
              </div>
              <div class="col-4 text-end ">
                <div class="icon icon-shape bg-gradient-primary shadow-primary text-center rounded-circle">
                  <i class="ni ni-money-coins  opacity-10" aria-hidden="true"></i>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
     
    </div>
    <div id="chart-container">
      FusionCharts will render here
  </div>
  </div>
<%- include('partials/footer') %>